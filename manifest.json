{
    "manifest_version": 2,
    "name": "Jon Lugg Launcher",
    "description": "Quick launch Jon Lugg Social links",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}